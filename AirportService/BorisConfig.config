<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.serviceModel>
    <services>
      <service name="AirTrafficController.TrafficController" behaviorConfiguration="mexBehavior">
        <!--<endpoint
          address="AirTrafficController.TrafficController"
          binding="basicHttpBinding"
          contract="AirTrafficController.ITrafficController">
        </endpoint>-->

        <endpoint
          address="AirTrafficController.TrafficController"
          binding="netTcpBinding"
          contract="AirTrafficController.ITrafficController">
        </endpoint>


        <endpoint
          address="AirTrafficController.TrafficController"
          binding="wsDualHttpBinding"
          contract="AirTrafficController.ITrafficController">
        </endpoint>

        <endpoint
          address="mex"
          binding="mexHttpBinding"
          contract="IMetadataExchange">
        </endpoint>

        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080"/>
            <add baseAddress="net.tcp://localhost:45617/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>